def div(k, j):
    return round(k/j, 2)

if __name__ == '__main__':
    while True:
            try:
                n1 = int(input('Digite um número: '))
                n2 = int(input('Digite um número: '))
                break
            except ValueError:
                print(f'Ocorreu um erro')

try: 
     r = div(n1, n2)
except ZeroDivisionError:
     print('Não é possivel por zero')
except:
     print('Ocorreu um erro')
else:
    print(f'Resultado: {r}')
finally:
     print('fim do calculo')
        


